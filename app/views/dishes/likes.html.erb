<% content_for :title, t('.title')%>
<h3 class='text-center fw-bold mt-5'><%= t('.title') %></h3>
<div class="container">
  <div class="row">
    <% if @likes.present? %>
      <%= render @likes %> <%# _dish.html.erbが呼び出される %>
    <% else %>
      <p><%= t('.no_data') %></p>
    <% end %>
    <div class='mt-4'>
      <%= paginate @likes %>
    </div>
  </div>
</div>